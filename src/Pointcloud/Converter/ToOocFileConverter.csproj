<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>fusee</AssemblyName>
    <PackageId>Fusee.Pointcloud.ToOocFileConverter</PackageId>
    <RootNamespace>Fusee.Tools.ToOocFileConverter</RootNamespace>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>fusee</ToolCommandName>
    <PackageTags>dotnet;tools;fusee</PackageTags>
  </PropertyGroup>
  <PropertyGroup>
    <OutputPath>$(BaseOutputPath)\Pointcloud\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(FuseeEngineRoot)\src\Pointcloud\PointAccessorCollection\Fusee.Pointcloud.PointAccessorCollections.csproj">
      <PrivateAssets>analyzers</PrivateAssets>
    </ProjectReference>  
  </ItemGroup>

</Project>
